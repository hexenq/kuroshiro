{
  "name": "kuroshiro",
  "version": "0.1.0",
  "description": "kuroshiro.js is a japanese language utility mainly for converting Kanji-mixed sentence to Hiragana, Katakana or Romaji with furigana and okurigana modes supported.",
  "main": "./dist/node/kuroshiro.js",
  "scripts": {
    "test": "mocha --timeout 10000 -R spec --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexenq/kuroshiro.js.git"
  },
  "keywords": [
    "japanese",
    "language",
    "convert",
    "converter",
    "kanji",
    "hiragana",
    "katakana",
    "romaji",
    "furigana",
    "okurigana",
    "utility",
    "tool"
  ],
  "author": {
    "name": "Hexen Qi",
    "email": "hexenq@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hexenq/kuroshiro.js/issues"
  },
  "homepage": "https://github.com/hexenq/kuroshiro.js",
  "dependencies": {
    "kuromoji": "^0.0.5",
    "wanakana": "^1.3.7"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-istanbul": "^0.10.3",
    "gulp-jsdoc": "^0.1.5",
    "gulp-jshint": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-webserver": "^0.9.1",
    "jshint": "^2.9.1-rc2",
    "mocha": "^2.3.4",
    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
